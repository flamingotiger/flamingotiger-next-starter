{
  "name": "nextjs-custom-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node --project tsconfig.server.json server",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node .build/server",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "validate": "yarn run prettier:check && yarn run lint:fix && yarn run lint:eslint",
    "prettier:check": "prettier --write --config ./.prettierrc './{src,pages}/**/*.{ts,tsx}'",
    "lint:eslint": "eslint './{src,pages}/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './{src,pages}/**/*.{ts,tsx}'",
    "lint:style": "stylelint './{src,pages}/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-plugin-styled-components": "^1.10.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "i18next": "^19.3.3",
    "i18next-icu": "^1.3.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-svgo": "^7.1.0",
    "next": "9.3.2",
    "next-i18next": "^4.2.1",
    "next-optimized-images": "^2.5.5",
    "next-redux-wrapper": "^5.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typesafe-actions": "^5.1.0",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/jest": "^25.1.4",
    "@types/next": "^9.0.0",
    "@types/node": "^13.7.2",
    "@types/react": "^16.9.20",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^4.4.3",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "jest": "^25.1.0",
    "jest-styled-components": "^7.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "serverless-dotenv-plugin": "^2.3.2",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.7.5",
    "webpack-node-externals": "^1.7.2"
  }
}
