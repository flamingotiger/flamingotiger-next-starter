{
	"name": "nextjs-custom-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "nodemon",
		"build": "next build && tsc --project tsconfig.server.json",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"prettier": "prettier --write --config ./.prettierrc './src/**/*.{ts,tsx}'",
		"lint": "eslint './src/**/*.{ts,tsx}'",
		"lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
		"build:analyzer": "ANALYZE=true npm run build",
		"git": "bash ./git.sh",
		"git:file": "bash ./git_file.sh",
		"validate": "npm run prettier && npm run lint && npm run build"
	},
	"dependencies": {
		"@next/bundle-analyzer": "^9.3.5",
		"axios": "^0.19.2",
		"babel-plugin-styled-components": "^1.10.6",
		"cross-env": "^7.0.2",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-optipng": "^7.1.0",
		"imagemin-svgo": "^7.1.0",
		"next": "latest",
		"next-offline": "^5.0.1",
		"next-optimized-images": "^2.5.8",
		"next-redux-saga": "^4.1.2",
		"next-redux-wrapper": "^5.0.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-hook-form": "4.10.2",
		"react-is": "16.13.1",
		"react-redux": "7.2.0",
		"react-sizes": "2.0.0",
		"redux": "4.0.5",
		"redux-saga": "1.1.3",
		"styled-components": "5.1.0",
		"styled-reset": "4.1.3",
		"typesafe-actions": "5.1.0",
		"uuid": "3.4.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/next-redux-saga": "^3.0.2",
		"@types/node": "13.13.2",
		"@types/react": "16.9.34",
		"@types/react-dom": "^16.9.8",
		"@types/react-redux": "^7.1.7",
		"@types/react-sizes": "^2.0.0",
		"@types/styled-components": "5.1.0",
		"@types/uuid": "3.4.8",
		"@typescript-eslint/eslint-plugin": "2.29.0",
		"@typescript-eslint/parser": "2.29.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-config-prettier": "6.11.0",
		"eslint-plugin-import": "2.20.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "3.1.3",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-react-hooks": "1.7.0",
		"nodemon": "2.0.3",
		"prettier": "1.19.1",
		"ts-node": "8.9.0",
		"typescript": "3.8.3",
		"webpack": "4.43.0"
	}
}
